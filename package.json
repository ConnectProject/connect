{
  "name": "connect",
  "version": "1.0.0",
  "description": "An open platform to save anonymous data coming from any application using this API",
  "scripts": {
    "start": "node src/index.js",
    "start.sandbox": "node src/sandbox.js",
    "build": "webpack --mode production",
    "dev": "webpack --mode development",
    "eslint.check": "eslint --ext js,jsx .",
    "eslint.fix": "eslint --ext js,jsx --fix .",
    "prettier.check": "prettier --list-different .",
    "prettier.fix": "prettier --write .",
    "test": "jest --config spec/jest.config.js -i --forceExit",
    "copyToSandbox": "./bin/migration.sh",
    "performance": "node bin/performance.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "printWidth": 80
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "Matters",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "axios": "0.21.1",
    "cors": "^2.8.5",
    "debug": "4.1.1",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "glob": "7.1.4",
    "js-cookie": "2.2.0",
    "jsonwebtoken": "8.5.1",
    "jwt-decode": "2.2.0",
    "lodash.flow": "^3.5.0",
    "moment": "^2.24.0",
    "mongoose": "5.7.5",
    "parse-dashboard": "1.3.3",
    "parse-server": "3.7.0",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-moment": "^0.9.2",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "request-promise": "4.2.4",
    "rxjs": "^6.5.2",
    "swagger-ui-express": "4.0.7",
    "uuid": "3.3.2",
    "validate.js": "0.13.1",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.5.0",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.5.0",
    "@babel/preset-react": "7.0.0",
    "@shelf/jest-mongodb": "^1.1.0",
    "babel-loader": "8.0.6",
    "core-js": "3.1.4",
    "css-loader": "5.0.1",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4",
    "html-webpack-plugin": "4.5.1",
    "husky": ">=4",
    "jest": "24.8.0",
    "json2csv": "^4.5.2",
    "lint-staged": ">=10",
    "mini-css-extract-plugin": "1.3.3",
    "nock": "^13.0.5",
    "node-sass": "4.13.1",
    "prettier": "^2",
    "sass-loader": "10.1.0",
    "style-loader": "0.23.1",
    "url-loader": "4.1.1",
    "webpack": "5.12.3",
    "webpack-cli": "4.3.1",
    "webpack-dev-server": "^3.7.2"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix",
    "*.*": "prettier --ignore-unknown --write"
  }
}
